<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="200.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CartController">
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="281.0" layoutY="165.0" mnemonicParsing="false" onAction="#handleCloseButtonAction" text="Cancel" />
            <ListView fx:id="cartList" layoutX="15.0" layoutY="29.0" prefHeight="134.0" prefWidth="318.0" />
            <Button layoutX="14.0" layoutY="2.0" mnemonicParsing="false" onAction="#btnShowList" text="Show all products from cart" />
            <Button layoutX="15.0" layoutY="165.0" mnemonicParsing="false" onAction="#btnTotalPrice" text="Total price" />
            <Label fx:id="totalPrice" layoutX="95.0" layoutY="165.0" prefHeight="24.0" prefWidth="70.0">
               <font>
                  <Font size="14.0" />
               </font></Label>
         </children>
      </Pane>
   </center>
</BorderPane>
